import java.nio.file.ClosedDirectoryStreamException;
import java.util.Scanner;

public class App {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int n,m;

        System.out.println("Enter two numbers ");
        n = sc.nextInt();
        m = sc.nextInt();

        while(m >= n){
            if(m %2 == 0)
                System.out.print(m + " " );
            m--;
        }

    }
}
